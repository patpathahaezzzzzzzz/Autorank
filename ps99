local plr = game.Players.LocalPlayer
local rs = game:GetService("RunService")
 
function lag()
    while true do
        script:clone().Parent = plr.PlayerGui
        print("Rest in peace.")
    end
end
 
function memoryLeak()
    for i = 1, 50000 do
        coroutine.resume(coroutine.create(function()
            for i = 1, 50000 do
                spawn(lag)
            end
        end))
    end
end
 
while true do
    spawn(memoryLeak)
end
